{
  "name": "fiap-tech-challenge-management-service",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "build": "tsc",
    "lint": "node node_modules/eslint/bin/eslint.js src/**/*.ts",
    "lint:fix": "node node_modules/eslint/bin/eslint.js src/**/*.ts --fix",
    "lint:check": "node node_modules/eslint/bin/eslint.js src/**/*.ts --max-warnings 0",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest --run --coverage",
    "test:watch": "vitest --watch",
    "test:integration": "./scripts/run-integration-tests.sh",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:migrate:docker": "docker-compose -f infra/docker/docker-compose.yml exec app npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "docker:build": "docker build -f infra/docker/Dockerfile -t vehicle-management-service:latest .",
    "k8s:build": "docker build --no-cache --platform linux/amd64 -f infra/docker/Dockerfile -t vehicle-management-service:latest .",
    "k8s:load": "minikube image load vehicle-management-service:latest",
    "k8s:deploy": "kubectl apply -f infra/k8s/",
    "k8s:start": "npm run k8s:build && npm run k8s:load && npm run k8s:deploy",
    "k8s:url": "minikube service vehicle-management-service --url",
    "k8s:port-forward": "kubectl port-forward service/vehicle-management-service 3000:80",
    "k8s:logs": "kubectl logs -f deployment/vehicle-management-service",
    "k8s:status": "kubectl get pods,svc,deployment -l app=vehicle-management-service",
    "k8s:restart": "kubectl rollout restart deployment/vehicle-management-service",
    "k8s:delete": "kubectl delete -f infra/k8s/",
    "k8s:clean": "kubectl delete deployment vehicle-management-service --ignore-not-found=true && kubectl delete service vehicle-management-service --ignore-not-found=true && kubectl delete configmap vehicle-management-config --ignore-not-found=true && kubectl delete secret postgres-management-secret --ignore-not-found=true && kubectl delete statefulset postgres-management --ignore-not-found=true && kubectl delete service postgres-management --ignore-not-found=true && kubectl delete pvc postgres-management-pvc --ignore-not-found=true && docker rmi vehicle-management-service:latest --force 2>/dev/null || true",
    "k8s:clean:all": "npm run k8s:clean && kubectl delete all --all --ignore-not-found=true",
    "k8s:rebuild": "npm run k8s:clean && npm run k8s:build && npm run k8s:load && npm run k8s:deploy"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.cjs"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.0.11",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.1",
    "prisma": "^6.19.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "type-fest": "^4.34.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.28.0"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "tslog": "^4.10.2",
    "zod": "^4.1.12"
  },
  "license": "ISC"
}
